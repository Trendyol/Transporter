{
  "TransferJobSettings": [
    {
      "Name": "Example Transfer Job Name",
      "Cron": "0/20 * * ? * *",
      "Source": {
        "Type": "postgresql",
        "Host": "example-source-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "example",
          "Table": "example_source_table",
          "IdColumn": "id"
        }
      },
      "Interim": {
        "Type": "mssql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "[Example]",
          "Table": "[ExampleInterimTable]",
          "BatchQuantity": 1000,
          "DataSourceName": "example.example_source_table"
        }
      },
      "Target": {
        "Type": "mssql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "[ExampleTarget]",
          "Table": "[ExampleTargetTable]",
          "IdColumn": "Id"
        }
      }
    }
  ],
  "PollingJobSettings": [
    {
      "Name": "Example Polling Job Name",
      "Cron": "0/20 * * ? * *",
      "Source": {
        "Type": "postgresql",
        "Host": "example-source-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "example",
          "Table": "example_source_table",
          "BatchQuantity": 1000,
          "IdColumn": "id",
          "Condition": "example_date_time_column < NOW() - INTERVAL '30 days'"
        }
      },
      "Target": {
        "Type": "mssql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "[ExampleTarget]",
          "Table": "[ExampleTargetTable]"
        }
      }
    }
  ]
}