{
  "TransferJobSettings": [
    {
      "Name": "Example Transfer Job Name",
      "Cron": "0/20 * * ? * *",
      "Source": {
        "Type": "mssql",
        "Host": "example-source-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "[Example]",
          "Table": "[ExampleSourceTable]",
          "IdColumn": "Id"
        }
      },
      "Interim": {
        "Type": "postgresql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "example",
          "Table": "example_interim_table",
          "BatchQuantity": 1000,
          "DataSourceName": "[Example].[ExampleSourceTable]"
        }
      },
      "Target": {
        "Type": "postgresql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "example_target",
          "Table": "example_target_table",
          "IdColumn": "id"
        }
      }
    }
  ],
  "PollingJobSettings": [
    {
      "Name": "Example Polling Job Name",
      "Cron": "0/20 * * ? * *",
      "Source": {
        "Type": "mssql",
        "Host": "example-source-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "[Example]",
          "Table": "[ExampleSourceTable]",
          "BatchQuantity": 1000,
          "IdColumn": "Id",
          "Condition": "ExampleDateTimeColumn < GETDATE()"
        }
      },
      "Target": {
        "Type": "postgresql",
        "Host": "example-target-host",
        "Options": {
          "ConnectionString": "",
          "Schema": "example_target",
          "Table": "example_target_table"
        }
      }
    }
  ]
}